<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE components PUBLIC "-//SEASAR//DTD S2Container 2.4//EN"
  "http://www.seasar.org/dtd/components24.dtd">
<components>
  <include path="ymir-convention.dicon" />
  <include path="mapping.dicon" />
  <component
    class="org.seasar.framework.container.autoregister.ComponentAutoRegister">
    <initMethod name="addReferenceClass">
      <arg>@org.seasar.ymir.Ymir@class</arg>
    </initMethod>
    <initMethod name="addClassPattern">
      <arg>"org.seasar.ymir.response.constructor.impl"</arg>
      <arg>".*ResponseConstructor"</arg>
    </initMethod>
    <initMethod name="addIgnoreClassPattern">
      <arg>"org.seasar.ymir.response.constructor.impl"</arg>
      <arg>"Abstract.*"</arg>
    </initMethod>
    <initMethod name="addClassPattern">
      <arg>"org.seasar.ymir.response.scheme.impl"</arg>
      <arg>".*Strategy"</arg>
    </initMethod>
    <initMethod name="addIgnoreClassPattern">
      <arg>"org.seasar.ymir.response.scheme.impl"</arg>
      <arg>"Abstract.*"</arg>
    </initMethod>
    <initMethod name="addClassPattern">
      <arg>"org.seasar.ymir.constraint.impl"</arg>
      <arg>".*Constraint"</arg>
    </initMethod>
    <initMethod name="addIgnoreClassPattern">
      <arg>"org.seasar.ymir.constraint.impl"</arg>
      <arg>"Abstract.*"</arg>
    </initMethod>
    <initMethod name="registerAll" />
  </component>
  <component
    class="org.seasar.ymir.container.RequestComponentDefFactory" />
  <component name="ymir" class="org.seasar.ymir.impl.YmirImpl">
    <property name="lifecycleListeners" bindingType="none">
      <meta name="delayed">
        @org.seasar.cms.pluggable.util.PluggableUtils@findAscendantComponents(container,
        @org.seasar.ymir.LifecycleListener@class)
      </meta>
    </property>
  </component>
  <component name="requestProcessor"
    class="org.seasar.ymir.impl.DefaultRequestProcessor">
    <property name="updaters" bindingType="none">
      <meta name="delayed">
        @org.seasar.cms.pluggable.util.PluggableUtils@findAscendantComponents(container,
        @org.seasar.ymir.Updater@class)
      </meta>
    </property>
  </component>
  <component name="responseProcessor"
    class="org.seasar.ymir.impl.DefaultResponseProcessor" />
  <component name="exceptionProcessor"
    class="org.seasar.ymir.impl.DefaultExceptionProcessor" />
  <component
    class="org.seasar.ymir.response.constructor.impl.ResponseConstructorSelectorImpl" />
  <component
    class="org.seasar.ymir.response.scheme.impl.StrategySelectorImpl" />
  <component class="org.seasar.ymir.impl.ApplicationManagerImpl" />
  <!-- SCOPE -->
  <component name="applicationScope"
    class="org.seasar.ymir.scope.impl.ApplicationScope" />
  <component name="sessionScope"
    class="org.seasar.ymir.scope.impl.SessionScope" />
  <component name="requestScope"
    class="org.seasar.ymir.scope.impl.RequestScope" />
  <component name="conversationScope"
    class="org.seasar.ymir.conversation.impl.ConversationScope" />
</components>
