<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE components PUBLIC "-//SEASAR//DTD S2Container 2.4//EN"
  "http://www.seasar.org/dtd/components24.dtd">
<components>
  <include path="global.dicon" />
  <meta name="expand">"org/seasar/ymir/ymir-creator.dicon"</meta>
  <component class="org.seasar.ymir.convention.YmirNamingConvention">
    <initMethod name="addRootPackageName">
      <arg>config.getProperty("rootPackageName", "skeleton")</arg>
    </initMethod>
    <initMethod name="addIgnorePackageName">
      <arg>config.getProperty("ignorePackageNames", "")</arg>
    </initMethod>
  </component>
</components>
